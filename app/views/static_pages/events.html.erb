


<% @events.each do |event| %>
  

  <h1><%= event["name"]%></h1>
  <h1><%= Date.parse(event["start_time"]).strftime("%-d %B %Y")%></h1>
  <div class="event">
  <img><%= image_tag event["cover"]["source"]%></img>
</div>
  <p><%= event["place"]["name"]%>, <span>starts at: <%= event["start_time"]%></span> 
  </p>
  
  <p><%= event["description"]%></p>
  

<% end %>


